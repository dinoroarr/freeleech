@go.Package { name = "github.com/dinoroarr/tracker-info/gen/FIOneTime" }
module main.trackerInfo.gen.FIOneTime

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.11.1#/go.pkl"

import "./TrackerInfo.pkl"

// The short code of the tracker
tracker: String

type: "one-time"
// ISO 8601
time: String(matches(Regex("^([0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2})/([0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2})$")))
// Timezone, optionally, by default UTC
timeZone: String?
// Whether double upload is set
doubleUpload: Boolean?
// Why freeleech is happening
// scheduled - Event scheduled normally
// holiday - Holiday freeleech (e.g. Christmas, Easter)
// donation-reached - Donation goal reached
// user-goal-reached - User goal reached (e.g. user tipped freeleech pool, freeleech bought, etc.)
// distraction - Distraction freeleech (e.g. domain change)
// other - Other reason
reason: ("scheduled" | "holiday" | "donation-reached" | "user-goal-reached" | "distraction" | "other")?

